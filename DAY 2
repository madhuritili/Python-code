#i/p="roses are red"  o/p="roses_are_red"
a="roses are red"
i=0
c=''
while i<len(a):
    if a[i]==' ':
        c=c+'_'
    else:
        c=c+a[i]
    i+=1
print(c)

# i/p=['hi','python']  o/p=[2,6]
a=['hi','python']
i=0
c=[]
while i<len(a):
    c=c+[len(a[i])]
    i+=1
print(c)

