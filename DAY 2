# a='2ew24cs040#' write a program to get the o/p no.=224040 , alpha='ewcs' , special='#'

a='2ew24cs040#'
num=""
alpha=""
special=""
i=0
while i<len(a):
    if '0'<=a[i]<='9':
        num=num+a[i]
    elif 'a'<=a[i]<='z' or 'A'<=a[i]<='Z':
        alpha=alpha+a[i]
    else:
        special=special+a[i]
    i+=1
print("numbers:",num)
print("alphabets:",alpha)
print("special character:",special)


# WRITE A PROGRAM TO GET THE TOTAL NO. OF ALPHABETS, NO. AND SPECIAL CHARACTERS IN A GIVEN STRING
a='2ew24cs040#'
num=""
alpha=""
special=""
i=0
num_count=""
alpha_count=""
special_count=""
while i<len(a):
    if '0'<=a[i]<='9':
        num=num.count(a[i])
    elif 'a'<=a[i]<='z' or 'A'<=a[i]<='Z':
        alpha=alpha.count(a[i])
    else:
        special=special.count(a[i])
    i+=1
print("numbers:",num)
print("alphabets:",alpha)
print("special character:",special)

a='2ew24cs040#'
num=""
alpha=""
special=""
i=0
num_count=0
alpha_count=0
special_count=0
while i<len(a):
    if '0'<=a[i]<='9':
        num=num+a[i]
        num_count+=1
    elif 'a'<=a[i]<='z' or 'A'<=a[i]<='Z':
        alpha=alpha+a[i]
        alpha_count+=1
    else:
        special=special+a[i]
        special_count+=1
    i+=1
print("numbers:",num_count)
print("alphabets:",alpha_count)
print("special character:",special_count)

# write a program to extract only special characters present inside a given string

a='2ew24cs040#'
special=""
i=0
while i<len(a):
    if not  ('0'<=a[i]<='9' or 'A'<=a[i]<='z' or 'a'<=a[i]<='z'):
        special=special+a[i]
    i+=1
print(special)

a='2ew24cs040#09"'
special=""
i=0
while i<len(a):
    if not  ('0'<=a[i]<='9' or 'A'<=a[i]<='z' or 'a'<=a[i]<='z'):
        special=special+a[i]
        print(special)
    i+=1


