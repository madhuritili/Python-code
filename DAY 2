#a=['python','sql']  o/p=['python':6, 'sql':3]

a=['python', 'sql']
i=0
c={}
while i<len(a):
    c[a[i]]=len(a[i])
    i+=1
print(c)


#factorial of a no.
n=4
f=1
for i in range(1,n+1):
    f=f*i
print(f)

# a='Hi'  e=['Python','sql']   o/p=['H':'Python','i':'sql']

a="hi"
e=['Python','sql']
i=0
c={}
while i<len(a) and i<len(e):
    c[a[i]]=e[i]
    i+=1
print(c)


#WRITE A PROGRAM TO IDENTIFY THE GIVEN NO. IS PERFECT NO. OR NOT(PERFECT NO. MEANS IF U CONSIDER NO. 6, CONSIDER THE NO. BELOW 6, THAT IS 1,2,3,4,5.
#DIVIDE 6 BY THESE FIVE NO. WHICH GIVES % 0, 6%2==0 ETC,. ONLY 1,2,3 GIVES MODULOS 0, ADD 1+2+3=6 U GOT 6 AGAIN, SO 6 IS A PERFECT NO.

n=6
i=1
sum=0
while i<n:
    if n%i==0:
        sum=sum+i
    i=i+1
if sum==n:
    print("perfect number")
else:
    print("not perfect number")


#identify given no.is prime or not

i=2
n=6
if n<2:
    print("not prime")
while i<n:
    if n%i==0:
        print("not prime")
        break
    i=i+1
else:
    print("prime")
